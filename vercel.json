{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/client/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install"
}